server {
    listen 80;
    listen [::]:80;

    server_name short-final-backend.tmuro17.xyz;

    location / {
        proxy_pass http://localhost:4800;
    }

    location /socket {
        proxy_pass http://localhost:4850;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
    }
}
